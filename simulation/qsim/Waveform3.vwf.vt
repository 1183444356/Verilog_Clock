`timescale 1 ps/ 1 ps
module test_vlg_sample_tst(
	cin,
	clk,
	data,
	load,
	reset,
	sampler_tx
);
input  cin;
input  clk;
input [7:0] data;
input  load;
input  reset;
output sampler_tx;

reg sample;
time current_time;
always @(cin or clk or data or load or reset)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module test_vlg_check_tst (
	cout,
	qout,
	sampler_rx
);
input  cout;
input [7:0] qout;
input sampler_rx;

reg  cout_expected;
reg [7:0] qout_expected;

reg  cout_prev;
reg [7:0] qout_prev;

reg  cout_expected_prev;
reg [7:0] qout_expected_prev;

reg  last_cout_exp;
reg [7:0] last_qout_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:2] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 2'b1;
end

// update real /o prevs

always @(trigger)
begin
	cout_prev = cout;
	qout_prev = qout;
end

// update expected /o prevs

always @(trigger)
begin
	cout_expected_prev = cout_expected;
	qout_expected_prev = qout_expected;
end


// expected qout[ 7 ]
initial
begin
	qout_expected[7] = 1'bX;
	qout_expected[7] = #999000 1'b0;
end 
// expected qout[ 6 ]
initial
begin
	qout_expected[6] = 1'bX;
	qout_expected[6] = #999000 1'b0;
end 
