module test_vlg_vec_tst();
// constants                                           
// general purpose registers
reg cin;
reg clk;
reg [7:0] data;
reg load;
reg reset;
// wires                                               
wire cout;
wire [7:0] qout;

wire sampler;                             

// assign statements (if any)                          
test i1 (
// port map - connection between master ports and signals/registers   
	.cin(cin),
	.clk(clk),
	.cout(cout),
	.data(data),
	.load(load),
	.qout(qout),
	.reset(reset)
);

// clk
always
begin
	clk = 1'b0;
	clk = #10000 1'b1;
	#10000;
end 

// reset
initial
begin
	reset = 1'b0;
	reset = #100000 1'b1;
	reset = #20000 1'b0;
end 

// load
